RewriteEngine On
Options FollowSymLinks
RewriteBase /meme/
RewriteRule ^meme/(.*) /comment.php?meme_id=$1
RewriteRule ^share/(.*) /comment.php?meme_id=$1&share=1
RewriteRule ^click/([0-9]+)/(.*) /redirect.php?meme_id=$1
RewriteRule ^tag/(.*) /memes_by_tag.php?tag_name=$1
RewriteRule ^cat/(.*) /show_cat.php?cat_name=$1
RewriteRule ^user/(.*) /profile.php?user_name=$1
RewriteRule ^votes/(.*) /profile.php?user_name=$1&view=v
RewriteRule ^comments/(.*) /profile.php?user_name=$1&view=c

