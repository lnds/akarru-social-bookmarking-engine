RewriteEngine On
Options FollowSymLinks
RewriteBase /meme/
RewriteRule ^meme/(.*) /comment.php?meme_id=$1
RewriteRule ^share/(.*) /comment.php?meme_id=$1&share=1
RewriteRule ^click/([0-9]+)/(.*) /redirect.php?meme_id=$1
RewriteRule ^tag/(.*) /memes_by_tag.php?tag_name=$1
RewriteRule ^cat/(.*) /show_cat.php?cat_name=$1
RewriteRule ^user/(.*) /profile.php?user_name=$1
RewriteRule ^votes/(.*) /profile.php?user_name=$1&view=v
RewriteRule ^comments/(.*) /profile.php?user_name=$1&view=c
RewriteRule ^send-meme/(.*) /sendlink.php?meme_id=$1
RewriteRule ^edit-meme/(.*) /edit_meme.php?meme_id=$1
RewriteRule ^edit-profile/(.*) /profile_edit.php?user_id=$1
RewriteRule ^tag-meme/(.*) /tag_meme.php?meme_id=$1



ErrorDocument 404 /404.php
ErrorDocument 403 /403.php

# No PHPSESSION appended to the URL
php_value session.use_only_cookies 1 
php_value session.use_trans_sid    0

#php_value mbstring.http_input auto
#php_value mbstring.http_output UTF-8
#php_value mbstring.internal_encoding UTF-8

